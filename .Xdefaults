*font            : -*-envypn-medium-*-normal-*-13-*-*-*-*-*-*-*
*boldFont        : -*-envypn-medium-*-normal-*-13-*-*-*-*-*-*-*

*title           : terminal
*buffered        : true
*geometry        : 160x50
*transparent     : false
*scrollBar       : false
*secondaryScroll : true
*pointerBlank    : true
*urgentOnBell    : true
*modifier        : alt
*shading         : 100
*saveLines       : 10000
*cursorBlink     : false
*dynamicColors   : on
*jumpScroll      : true

*perl-ext        : default,matcher
*perl-lib        : /usr/lib/*perl5/
*url-launcher    : firefox
*matcher.button  : 1

*cursorColor     : #80724e
*cursorColor2    : #fff
*colorUL         : #fea63c
*cursorUnderline : true

! colorscheme
*foreground      : #ddeedd
*background      : #000000

!black
*color0:         rgb:19/19/19
*color8:         rgb:25/25/25
!red
*color1:         rgb:80/32/32
*color9:         rgb:98/2b/2b
!green
*color2:         rgb:5b/76/2f
*color10:        rgb:89/b8/3f
!brown/yellow
*color3:         rgb:aa/99/43
*color11:        rgb:ef/ef/60
!blue
*color4:         rgb:32/4c/80
*color12:        rgb:2b/4f/98
!magenta
*color5:         rgb:70/6c/9a
*color13:        rgb:82/6a/b1
!cyan
*color6:         rgb:92/b1/9e
*color14:        rgb:a1/cd/cd
!white
*color7:         rgb:ff/ff/ff
*color15:        rgb:dd/dd/dd
