*font            : -*-tamsyn-medium-*-normal-*-15-*-*-*-*-*-*-*
*boldFont        : -*-tamsyn-bold-*-normal-*-15-*-*-*-*-*-*-*

*title           : terminal
*buffered        : true
*geometry        : 160x50
*transparent     : false
*scrollBar       : false
*secondaryScroll : true
*pointerBlank    : true
*urgentOnBell    : true
*modifier        : alt
*shading         : 100
*saveLines       : 10000
*cursorBlink     : false
*dynamicColors   : on
*jumpScroll      : true

*perl-ext        : default,matcher
*perl-lib        : /usr/lib/*perl5/
*url-launcher    : firefox
*matcher.button  : 1

*cursorColor     : #80724e
*cursorColor2    : #fff
*colorUL         : khaki

! colorscheme
*foreground      : #ddeedd
*background      : #000000
*color0          : #1c1c1c
*color8          : #4D4D4D
*color1          : #cd5c5c
*color9          : #a52a2a
*color2          : #b7ce42
*color10         : #bde077
*color3          : #ffe863
*color11         : #fea63c
*color4          : #66aabb
*color12         : #aaccbb
*color5          : #b7416e
*color13         : #bb4466
*color6          : #5e7175
*color14         : #a3babf
*color7          : #6c887a
*color15         : #ddeedd
