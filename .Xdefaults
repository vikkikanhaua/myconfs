! *font            : -*-tamsyn-medium-*-normal-*-14-*-*-*-*-*-*-*
! *boldFont        : -*-tamsyn-medium-*-normal-*-14-*-*-*-*-*-*-*

*font            : -*-envypn-*-*-*-*-13-*-*-*-*-*-*-*
*boldFont        : -*-envypn-*-*-*-*-13-*-*-*-*-*-*-*

*title           : terminal
*buffered        : true
*geometry        : 160x50
*transparent     : false
*scrollBar       : false
*secondaryScroll : true
*pointerBlank    : true
*urgentOnBell    : true
*modifier        : alt
*shading         : 100
*saveLines       : 10000
*cursorBlink     : false
*dynamicColors   : on
*jumpScroll      : true

*perl-ext        : default,matcher
*perl-lib        : /usr/lib/*perl5/
*url-launcher    : firefox
*matcher.button  : 1

*cursorColor2    : #fff
*colorUL         : #fea63c
*cursorUnderline : true

