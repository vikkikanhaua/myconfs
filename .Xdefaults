!*font            : -*-profont-medium-*-*-*-11-*-*-*-*-*-*-*
!*boldFont        : -*-profont-medium-*-*-*-11-*-*-*-*-*-*-*
!*font            : -*-montecarlo-medium-r-normal-*-11-*-*-*-*-*-*-*
!*boldFont        : -*-montecarlo-bold-r-normal-*-11-*-*-*-*-*-*-*
!*font            : -*-envypn-medium-r-*-*-13-*-*-*-*-*-*-*
!*boldFont        : -*-envypn-medium-r-*-*-13-*-*-*-*-*-*-*

*font            : -*-tamsyn-medium-*-normal-*-14-*-*-*-*-*-*-*
*boldFont        : -*-tamsyn-medium-*-normal-*-14-*-*-*-*-*-*-*

*title           : terminal
*buffered        : true
*geometry        : 160x50
*transparent     : false
*scrollBar       : false
*secondaryScroll : true
*pointerBlank    : true
*urgentOnBell    : true
*modifier        : alt
*shading         : 100
*saveLines       : 10000
*cursorBlink     : false
*dynamicColors   : on
*jumpScroll      : true

*perl-ext-common : default,matcher
*perl-lib        : /usr/lib/*perl/
*urlLauncher     : firefox
*matcher.button  : 1

*cursorColor     : #80724e
*cursorColor2    : #fff
*colorUL         : khaki

! colorscheme
*foreground      : #ddeedd
*background      : #000000

*color0          : #1c1c1c
*color8          : #4D4D4D
*color1          : #cd5c5c
*color9          : #a52a2a
*color2          : #b7ce42
